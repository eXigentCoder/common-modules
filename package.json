{
  "name": "@exigentcoder/common-modules",
  "version": "0.10.6",
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 6.0.0"
  },
  "description": "A collection of shared js modules used primarily on the server side.",
  "files": [
    "packages"
  ],
  "scripts": {
    "coverage": "nyc --require ./test-init.js mocha",
    "codecov": "codecov",
    "lint-ci": "eslint \"packages/**/*.js\"",
    "lint": "npm run lint-ci -- --fix",
    "test": "mocha",
    "watch": "npm run test -- --watch",
    "snyk": "snyk test",
    "prettier": "prettier --write \"packages/**/*.js\"",
    "update": "npm-check -Eu",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "preversion": "npm run lint-ci && npm run coverage && npm run snyk",
    "postversion": "node build/bit-tag && npm run bit-export && npm run bit-commit",
    "bit-add": "bit add packages/* --tests packages/{PARENT}/**/*.test.js",
    "bit-export": "bit export exigentcoder.common-modules",
    "bit-commit": "git add . && git commit -m \"[skip ci]Updating .bitmap file post tag\" && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eXigentCoder/common-modules.git"
  },
  "keywords": [
    "KrimZen-Ninja"
  ],
  "author": "Ryan Kotzen <ryan@krimzen.ninja> (http://ryankotzen.com/)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jsdoc": "4.8.4",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "mocha": "6.1.3",
    "node-mocks-http": "1.7.5",
    "npm-check": "5.9.0",
    "nyc": "14.0.0",
    "prettier": "1.8.2",
    "snyk": "1.52.0",
    "yargs": "13.2.2"
  },
  "dependencies": {
    "@hapi/boom": "7.4.2",
    "ajv": "6.10.0",
    "ajv-errors": "1.0.1",
    "better-ajv-errors": "0.6.4",
    "debug": "4.1.1",
    "express": "4.16.4",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "mongodb": "3.2.3",
    "nconf": "0.10.0",
    "pino": "5.12.3",
    "pino-pretty": "3.0.0",
    "pluralize": "7.0.0",
    "uuid": "3.3.2",
    "v8n": "1.2.3"
  }
}
