{
  "name": "@exigentcoder/common-modules",
  "version": "0.0.8",
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 6.0.0"
  },
  "description": "A collection of shared js modules used primarily on the server side.",
  "files": [
    "packages"
  ],
  "scripts": {
    "coverage": "nyc mocha",
    "codecov": "codecov",
    "lint-ci": "eslint \"packages/**/*.js\"",
    "lint": "npm run lint-ci -- --fix",
    "test": "mocha",
    "watch": "npm run test -- --watch",
    "snyk": "snyk test",
    "prettier": "prettier --write \"packages/**/*.js\"",
    "update": "npm-check -Eu",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "preversion": "npm run lint-ci && npm run coverage && npm run snyk",
    "postversion": "node build/bit-tag && git add . && git commit -m \"[skip ci]Updating .bitmap file post tag\" && git push --follow-tags && npm run bit-export",
    "bit-add": "bit add packages/* --tests packages/**/*.test.js",
    "bit-export": "bit export exigentcoder.common-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrimZen Ninja/krimzen-ninja-common-errors.git"
  },
  "keywords": [
    "KrimZen-Ninja"
  ],
  "author": "Ryan Kotzen <ryan@krimzen.ninja> (https://github.com/KrimzenNinja)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "mocha": "6.1.3",
    "npm-check": "5.9.0",
    "nyc": "14.0.0",
    "prettier": "1.8.2",
    "snyk": "1.52.0"
  },
  "dependencies": {
    "debug": "4.1.1"
  }
}
